<Application
    x:Class="BiciMAD_Map.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BiciMAD_Map"
    xmlns:viewmodel="using:BiciMAD_Map.ViewModels"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <viewmodel:VMLocator x:Key="vmLocator"></viewmodel:VMLocator>
            <x:String x:Key="MapServiceToken">BING-MAPS-TOKEN-GOES-HERE</x:String>
            <SolidColorBrush x:Key="SystemControlHighlightAccentBrush" Color="#FF6138"/>
            <Style x:Key="EllipsisButton" TargetType="Button">
                <Setter Property="Visibility" Value="Collapsed"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>

</Application>
